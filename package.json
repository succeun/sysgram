{
	"name": "sysgram.js",
	"version": "0.1.0",
	"description": "Sysgram.js draw the system architecture using JavaScript.",
	"main": "dist/sysgram.bundle.js",
	"unpkg": "dist/sysgram.min.bundle.js",
	"files": [
		"dist"
	],
	"scripts": {
		"docs": "docsify serve ./docs",
		"dev": "http-server -p 40000",
		"build": "webpack && npm run copy:wasm",
		"copy:wasm": "cpx -p node_modules/d3-graphviz/node_modules/@hpcc-js/wasm/dist/graphvizlib.wasm ./dist"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/succeun/sysgram.js"
	},
	"keywords": [
		"d3",
		"graphviz",
		"DOT",
		"graph layout",
		"diagrams",
		"diagram",
		"Viz.js",
		"cloud"
	],
	"author": "Jeong-Ho, Eun",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/succeun/sysgram.js/issues"
	},
	"homepage": "https://succeun.github.io/sysgram.js",
	"dependencies": {
		"canvg": "3.0.7",
		"d3": "6.7.0",
		"d3-graphviz": "4.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.14.6",
		"@babel/preset-env": "^7.14.5",
		"babel-loader": "^8.2.2",
		"clean-webpack-plugin": "^4.0.0-alpha.0",
		"cpx": "^1.5.0",
		"docsify": "^4.4.3",
		"http-server": "^0.12.3",
		"webpack": "^5.39.1",
		"webpack-cli": "^4.7.2"
	}
}
